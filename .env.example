# Frontend Environment Variables Template
# Copy this file to .env and fill in your actual values

# Development
NODE_ENV=development
PORT=3000

# Backend API Configuration
REACT_APP_API_URL=http://localhost:5000/api

# Analytics (Optional)
GOOGLE_ANALYTICS_ID=your_ga_id_here

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_CONTACT_FORM=true

# For production deployment, update REACT_APP_API_URL to your deployed backend URL
# Example: REACT_APP_API_URL=https://your-backend.railway.app/api

# Contact Information
REACT_APP_COMPANY_EMAIL=ceo@gideonstechnology.com
REACT_APP_COMPANY_PHONE=+234-xxx-xxx-xxxx

# Social Media Links
REACT_APP_LINKEDIN_URL=https://linkedin.com/in/your-profile
REACT_APP_COMPANY_WEBSITE=https://gideonstechnology.com
